{
  "name": "mapbox-render",
  "version": "0.0.1",
  "description": "Render PNGs from Mapbox style",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "start": "npm run static-image",
    "build:watch": "tsc -p tsconfig.json --watch",
    "example:watch": "tsc -p tsconfig.dist.json --watch",
    "static-image": "node example/static-image.js",
    "coverage": "nyc jest",
    "coverage:mocha": "nyc mocha -r ts-node/register test/**/render.test.ts",
    "test:mocha": "mocha --exit --reporter spec -r ts-node/register test/**/render.test.ts",
    "test": "jest --coverage"
  },
  "bin": {},
  "dependencies": {
    "@mapbox/mapbox-gl-native": "^4.1.0",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.7",
    "url-parse": "^1.4.3",
    "sharp": "^0.22.0"
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/jest": "^24.0.12",
    "@types/pixelmatch": "^4.0.0",
    "@types/request-promise-native": "^1.0.7",
    "@types/sharp": "^0.22.0",
    "@types/url-parse": "^1.4.3",
    "chai": "^4.2.0",
    "dts-gen": "^0.5.7",
    "jest": "^24.8.0",
    "jsdoc": "^3.5.5",
    "pixelmatch": "^4.0.2",
    "source-map-support": "^0.5.12",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.1.0",
    "tslib": "^1.9.3",
    "typescript": "^3.4.5"
  },
  "repository": "https://github.com/henrythasler/mapbox-render"
}
